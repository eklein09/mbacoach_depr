<% schoolList = [ "hbs","stanford","wharton","booth","sloan",
                                       "kellogg","haas","tuck",
                                       "darden", "fuqua","columbia",
                                       "anderson","stern","yalesom","ross" ] %>
<% schoolList = schoolList.zip(@d) %>

<dl class="dl-horizontal">



<% schoolList.each do |schoolName| %>
  <% if schoolName[0]=="hbs" then %>
    <dt><%= schoolName[0].upcase %></dt>
  <% else %>
  <dt><%= schoolName[0].titlecase %></dt>
  <% end %>
  <dd><%= (schoolName[1].to_f*100).round %>%</dd>
<% end %>

<!--       <dt>Stanford</dt>
      <dd> <%= @user.gmat %></dd> -->

<!--       <dt>Wharton</dt>
      <dd> <%= @user.gpa %></dd> -->

</dl>
